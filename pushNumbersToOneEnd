    1) pushZerosToEnd
    private static int[] moveZerosToRight(int[] a) {
        int count = 0;
        for (int i = 0; i < a.length; i++) {
            if (a[i] != 0) {
                a[count] = a[i];
                count++;
            }
        }
       while (count < a.length) {
            a[count] = 0;
            count++;

        }
        return a;
    }

